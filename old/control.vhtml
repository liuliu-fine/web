<script>
    var query = window.location.search.substr(1);
    var para = query.split("&");
    if (para[0].substr(2) == "c") {
        location.href = "_coupon.vhtml?" + para[1];
    } else if (para[0].substr(2) == "p") {
        location.href = "_point.vhtml?" + para[1];
    } if (para[0].substr(2) == "t") {
        location.href = "payRule.vhtml?" + para[1];

    }else {
        location.href = "index.vhtml";
    }

</script>
